{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Start Albus",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/index.js", // Adjust path if necessary
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
